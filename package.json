{
  "name": "snowpack-dev-test for mdn express tut using yarn v2",
  "scripts": {
    "start": "NODE_ENV=production node -r dotenv/config server.js",
    "start-dev": "nodemon --ignore public -r dotenv/config server.js",
    "watch-stylus": "stylus -w -m --sourcemap-base public --sourcemap-root /public public",
    "dev": "concurrently \"npm:start-dev\" \"npm:watch-stylus\""
  },
  "browserslist": [
    "last 2 versions",
    "> 0.5%",
    "node 10",
    "not dead"
  ],
  "devDependencies": {
    "browserslist": "^4",
    "concurrently": "^5.2.0",
    "faker": "^4.1.0",
    "nodemon": "^2.0.4",
    "parcel": "^1.12.4",
    "stylus": "^0.54.8",
    "snowpack": "^2",
    "snowpack-plugin-stylus": "^1"
  },
  "dependencies": {
    "bootstrap": "5.0.0-alpha2",
    "bootstrap-icons": "~1.1",
    "date-fns": "^2",
    "dotenv": "~8.2",
    "ejs": "~3.1",
    "express": "~4.17",
    "mongoose": "~5.10",
    "pug": "^3.0",
    "slugify": "^1.4.5"
  }
}
